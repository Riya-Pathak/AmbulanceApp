<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#455A64"
    tools:context="com.example.shubhamr.myfirstmapactivity.UserSens">

    <TextView
        android:id="@+id/userlattext"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/userlondtext"
        android:layout_below="@+id/aadhaartext"
        android:layout_marginTop="14dp"
        android:text="text"
        android:textSize="30sp" />

    <TextView
        android:id="@+id/userlondtext"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/username"
        android:layout_below="@+id/userlattext"
        android:layout_marginTop="16dp"
        android:text="text"
        android:textSize="30sp" />

    <TextView
        android:id="@+id/username"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/age"
        android:layout_below="@+id/userlondtext"
        android:layout_marginTop="16dp"
        android:text="text"
        android:textSize="30sp" />

    <Button
        android:id="@+id/navigate"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/emergency"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="77dp"
        android:background="@drawable/roundedbutton"
        android:text="Navigate" />

    <TextView
        android:id="@+id/age"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/username"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="17dp"
        android:text="text"
        android:textSize="30sp" />

    <TextView
        android:id="@+id/emergency"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/age"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="22dp"
        android:text="text"
        android:textSize="30sp" />

    <TextView
        android:id="@+id/aadhaartext"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginTop="48dp"
        android:text="text"
        android:textSize="30sp"
        android:layout_alignParentStart="true" />

</RelativeLayout>
